{
  "name": "@slowpost/client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "node ../scripts/generate-story-tests.mjs && vitest run --config vitest.config.ts",
    "test:watch": "node ../scripts/generate-story-tests.mjs && vitest watch --config vitest.config.ts",
    "test:stories": "node ../scripts/generate-story-tests.mjs && vitest run --config vitest.config.ts --include __generated_tests__/**/*.test.tsx"
  },
  "dependencies": {
    "next": "^14.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-links": "^9.1.10",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-storybook": "^9.1.10",
    "jsdom": "^24.0.0",
    "storybook": "^9.1.10",
    "typescript": "^5.3.3",
    "vite": "^5.4.10",
    "vitest": "^3.2.4"
  }
}
